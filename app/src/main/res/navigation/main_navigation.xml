            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/listRtFragment"
        android:name="com.pangper.infopangulahpermai.view.mainMenu.ListRtFragment"
        android:label="fragment_list_rt"
        tools:layout="@layout/fragment_list_rt" >

        <action
