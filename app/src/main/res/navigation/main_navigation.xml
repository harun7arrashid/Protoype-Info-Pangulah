        android:label="fragment_login_admin_dkm"
        tools:layout="@layout/fragment_login_admin_dkm" >

        <action
            android:id="@+id/action_loginAdminDkmFragment_to_listInfoDkmFragment"
            app:destination="@id/listInfoDkmFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/listInfoDkmFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
