<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".view.rt.rt04.infort04.InfoRt04Fragment">

    <Button
        android:id="@+id/btn_infoWarga_rt04"
        style="@style/mButton"
        android:background="@drawable/bg_btn3"
        android:text="@string/citizen_info_rt04"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3" />

    <Button
        android:id="@+id/btn_infoKas_rt04"
        style="@style/mButton"
        android:background="@drawable/bg_btn3"
        android:text="@string/money_info_rt04"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_infoWarga_rt04"/>

    <Button
        android:id="@+id/btn_kelolaData_rt04"
        style="@style/mButton"
        android:background="@drawable/bg_btn3"
        android:text="Kelola Data RT04"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_infoKas_rt04" />

</androidx.constraintlayout.widget.ConstraintLayout>